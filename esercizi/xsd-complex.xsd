<xsd:schema
        xmlns:xsd="http://www.w3c.org/2001/XMLSchema">
    <xsd:element name="Employee">
        <xsd:complexType>
            <!-- nome attributo associato ad elemento "employee" -->
            <xsd:attribute name="FirstName"/>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>
